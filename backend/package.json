{
  "name": "quizma-backend",
  "version": "1.0.0",
  "description": "Backend de l'application QuizMa pour la Chambre des Conseillers",
  "main": "src/app.js",
  "scripts": {
    "start": "NODE_ENV=production node src/app.js",
    "dev": "nodemon src/app.js",
    "prod": "pm2 start src/app.js --name quizma-backend",
    "prod:stop": "pm2 stop quizma-backend",
    "prod:restart": "pm2 restart quizma-backend",
    "prod:logs": "pm2 logs quizma-backend",
    "init-db": "node src/scripts/initDb.js"
  },
  "keywords": [
    "quiz",
    "parlement",
    "maroc",
    "conseillers"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.16.1",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "pm2": "^5.4.3"
  }
}
